  "scripts": {
    "dev": "tsnd --respawn --transpileOnly -r tsconfig-paths/register ./src/index.ts",
    "build": "ttsc",
    "start": "node ./node_modules/pm2/bin/pm2 startOrRestart ecosystem.config.js --name PrototypeServer --env production --log-date-format='YYYY-MM-DD HH:mm Z'",
