{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "lib": ["es5", "es6", "dom"],
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "Prototype/*": ["./src/*"],
        "Types": ["./src/types/index.ts"]
      }
  },
  "include": [
      "./src/**/*",
  ],
  "exclude": [
    "./node_modules/*"
  ]
}
